  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=config/webpack/prod",
    "clean-dist": "rm -f -r -d dist",
    "lint": "npm run lint:js && npm run lint:jsx && npm run lint:sass",
    "lint:js": "jshint ./src/**/*.js",
    "lint:jsx": "jsxhint ./src/**/*.jsx",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=config/webpack/dev",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watch --coverage --config=config/jest.json"
  },
